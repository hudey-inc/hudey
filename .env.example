# API keys and environment variables
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# Hudey agent
ANTHROPIC_API_KEY=

# Outreach email (Resend)
RESEND_API_KEY=
RESEND_WEBHOOK_SECRET=
# ^ Signing secret from Resend dashboard → Webhooks → Signing Secret (starts with whsec_)
OUTREACH_FROM_EMAIL=Brand <hello@yourdomain.com>
OUTREACH_REPLY_TO=hello@yourdomain.com

# Phyllo (creator discovery & monitoring)
# Use either API key (Bearer) or Client ID + Secret (Basic Auth)
PHYLLO_API_KEY=
PHYLLO_CLIENT_ID=
PHYLLO_CLIENT_SECRET=
PHYLLO_BASE_URL=https://api.insightiq.ai/v1

# Supabase (backend persistence)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_service_role_key
SUPABASE_JWT_SECRET=your_jwt_secret_from_dashboard

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# Example placeholders - add what your tools need:
# API_KEY=your_api_key_here
# GOOGLE_SHEETS_ID=your_sheet_id_here